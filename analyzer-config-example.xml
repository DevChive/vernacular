<?xml version="1.0" encoding="utf-8"?>
<vernacular-analyzer>
  <illegal-words>
    <word value="rdio">
      <except-in value="Copyright Â© 2012 Rdio, Inc."/>
    </word>
  </illegal-words>
  <supported-tags>
    <tag value="product_name"/>
    <tag value="website_url"/>
  </supported-tags>
  <spellcheck hunspell-affix="hunspell/en_US.aff"
              hunspell-dictionary="hunspell/en_US.dic">
    <dictionary-path value="custom-words-file"/>
  </spellcheck>
</vernacular-analyzer>
